{
    "name": "app",
    "version": "0.0.1",
    "license": "MIT",
    "browser": {
        "fs": false,
        "constants": false,
        "child_process": false,
        "tty": false
    },
    "scripts": {
        "bundle": "npx browserify --standalone CodeMirror --no-browser-field ./vendor/code-mirror.js -o ./vendor/code-mirror.compiled.js",
        "minify": "npx esbuild --minify ./vendor/code-mirror.compiled.js --outfile=./vendor/code-mirror.min.js"
    },
    "dependencies": {
        "@codemirror/basic-setup": "0.19.0",
        "@codemirror/autocomplete": "0.19.12",
        "@codemirror/legacy-modes": "0.19.0",
        "@codemirror/lint": "0.19.3",
        "@codemirror/state": "0.19.5",
        "@codemirror/stream-parser": "0.19.2",
        "@codemirror/theme-one-dark": "0.19.1",
        "@codemirror/view": "0.19.16"
    }
}
